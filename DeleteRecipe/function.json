{
  "bindings": [
    {
      "name": "req",
      "authLevel": "function",
      "methods": [
        "post"
      ],
      "direction": "in",
      "type": "httpTrigger"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "collection",
      "direction": "out",
      "type": "cosmosDB",
      "databaseName": "foodplannerdb",
      "collectionName": "recipes",
      "connectionStringSetting": "foodplannerdb_DOCUMENTDB"
    }
  ]
}