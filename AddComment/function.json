{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "newComment",
      "direction": "out",
      "type": "cosmosDB",
      "databaseName": "foodplannerdb",
      "collectionName": "comments",
      "connectionStringSetting": "foodplannerdb_DOCUMENTDB"
    },
    {
      "name": "outQueue",
      "direction": "out",
      "type": "queue",
      "queueName": "commentssums",
      "connection": "foodplannerqueue_STORAGE"
    }
  ]
}