{
  "bindings": [
    {
      "name": "req",
      "authLevel": "function",
      "methods": [
        "get"
      ],
      "direction": "in",
      "type": "httpTrigger"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "dbRecipe",
      "databaseName": "foodplannerdb",
      "collectionName": "recipes",
      "connectionStringSetting": "foodplannerdb_DOCUMENTDB",
      "sqlQuery": "SELECT c.id, c.title, c.author, c.imageName FROM c WHERE NOT IS_DEFINED(c.deleted) OR c.deleted != 1",
      "direction": "in",
      "type": "cosmosDB"
    }
  ]
}