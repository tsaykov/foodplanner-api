{
  "bindings": [
    {
      "name": "req",
      "route": "plans/{id}",
      "authLevel": "function",
      "methods": [
        "get"
      ],
      "direction": "in",
      "type": "httpTrigger"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "dbPlan",
      "databaseName": "foodplannerdb",
      "collectionName": "plans",
      "connectionStringSetting": "foodplannerdb_DOCUMENTDB",
      "sqlQuery": "SELECT * FROM c WHERE c.id={id}",
      "direction": "in",
      "type": "cosmosDB"
    }
  ]
}